/***************************************************************************																		****		CmpNPC															****		=============													****																		****		Compare NPCs bys aiv'ars										****														by Zysk			***************************************************************************/FUNC INT CmpNpc(VAR C_NPC P1,VAR C_NPC P2){	If(!Hlp_Isvalidnpc(P1)||!Hlp_Isvalidnpc(P2))	{		Return 0;	};	Var Int Old1;	Var Int Old2;	Var Int Result;	Old1=Npc_GetAivar(P1,0);	Old2=Npc_GetAivar(P2,0);	Npc_SetAivar(P1,0,0);	Npc_SetAivar(P2,0,1);	Result=Npc_GetAivar(P1,0);	Npc_SetAivar(P1,0,Old1);	Npc_SetAivar(P2,0,Old2);	Return Result;};